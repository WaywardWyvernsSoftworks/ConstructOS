{
  "name": "ConstructOS",
  "version": "0.8.3",
  "main": "dist-electron/main/index.js",
  "description": "Electron LLM React",
  "author": "Wayward Wyverns Softworks",
  "private": true,
  "build": {
    "publish": [
      {
        "provider": "github",
        "repo": "ConstructOS",
        "owner": "WaywardWyvernsSoftworks",
        "releaseType": "release",
        "private": false
      }
    ]
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build-win": "tsc && vite build && electron-builder --win",
    "build-mac": "tsc && vite build && electron-builder --mac",
    "build-linux": "tsc && vite build && electron-builder --linux",
    "build-all": "tsc && vite build && electron-builder --mac --win --linux",
    "release": "electron-builder --publish=always",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@azure/cognitiveservices-websearch": "^3.0.1",
    "@azure/ms-rest-azure-js": "^2.1.0",
    "@dqbd/tiktoken": "^1.0.7",
    "@google-ai/generativelanguage": "^1.0.0",
    "@material-tailwind/react": "^2.1.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@uiw/react-codemirror": "^4.21.9",
    "@uiw/react-md-editor": "^3.23.5",
    "@uiw/react-textarea-code-editor": "^2.1.7",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "azure-cognitiveservices-search": "^1.1.0-preview",
    "chromadb": "^1.5.5",
    "discord.js": "^14.12.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "exifr": "^7.1.3",
    "form-data": "^4.0.0",
    "google-auth-library": "^9.0.0",
    "intro.js": "^7.2.0",
    "intro.js-react": "^1.0.0",
    "lucide-react": "^0.265.0",
    "openai": "^3.3.0",
    "png-chunk-text": "^1.0.0",
    "png-chunks-encode": "^1.0.0",
    "png-chunks-extract": "^1.0.0",
    "postcss": "^8.4.26",
    "pouchdb": "^8.0.1",
    "pouchdb-adapter-leveldb": "^8.0.1",
    "python-shell": "^5.0.0",
    "react-icons": "^4.10.1",
    "react-markdown": "^8.0.7",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "react-router-hash-link": "^2.4.3",
    "react-select": "^5.7.4",
    "react-switch": "^7.0.0",
    "react-textarea-autosize": "^8.5.3",
    "remark-gfm": "^3.0.1",
    "serpapi": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "tw-elements": "^1.0.0-beta3",
    "twilio": "^4.15.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@playwright/test": "^1.32.0",
    "@types/node": "^20.4.8",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-hash-link": "^2.4.6",
    "@types/validator": "^13.11.1",
    "@types/websocket": "^1.0.6",
    "@vitejs/plugin-react": "^3.1.0",
    "electron": "^23.2.0",
    "electron-builder": "^24.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0",
    "tailwindcss-textshadow": "^2.1.3",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "keywords": []
}
